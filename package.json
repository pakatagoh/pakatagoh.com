    "highlight.js": "^11.4.0",
    "mdast": "^3.0.0",
    "mdx-bundler": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rehype-autolink-headings": "^6.1.1",
