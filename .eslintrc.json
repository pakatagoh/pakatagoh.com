{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "impliedStrict": true,
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "plugins": ["import"],
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "rules": {
    "no-console": "warn",
    "consistent-return": 0,
    "func-names": 0,
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "react/require-default-props": 0,
    "react/prop-types": 0,
    "react/jsx-props-no-spreading": 0,
    "react/jsx-one-expression-per-line": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ]
  }
}
